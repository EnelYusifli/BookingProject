}
@model HotelGetViewModel


			<!-- Page main content START -->
			<div class="page-content-wrapper p-xxl-4">

				<!-- Title -->
				<div class="row">
					<div class="col-12 mb-4 mb-sm-5">
						<div class="d-sm-flex justify-content-around align-items-center">
							<h1 class="h3 mb-2 mb-sm-0">Premium Room With Balcony</h1>
							<div>
				<a asp-action="refusehotel" asp-route-id="@Model.Id" class="btn btn-danger-soft text-nowrap mb-0"><i class="bi bi-x-circle-fill"></i> Refuse</a>
				<a asp-action="approvehotel" asp-route-id="@Model.Id" class="btn btn-success-soft text-nowrap mb-0"><i class="bi bi-check-circle-fill"></i> Approve</a>
							</div>
						</div>
					</div>
				</div>

				<!-- Booking detail START -->
				<div class="row g-4 g-xl-5">
					<!-- Image -->
					<div class="col-xxl-6">
						<div class="row g-2 g-sm-4">
							@foreach (var item in Model.ImageFileUrls)
				{
							<div class="col-6">
								<a data-glightbox data-gallery="gallery" href="@item">
									<div class="card card-element-hover card-overlay-hover overflow-hidden">
										<!-- Image -->
										<img src="@item" class="rounded-3" alt="">
										<!-- Full screen button -->
										<div class="hover-element w-100 h-100">
											<i class="bi bi-fullscreen fs-6 text-white position-absolute top-50 start-50 translate-middle bg-dark rounded-1 p-2 lh-1"></i>
										</div>
									</div>
								</a>
							</div>
				}
						</div>
					</div>

					<!-- Content -->
		<div class="col-xl-7 order-1">
			<div class="vstack gap-5">

				<!-- About hotel START -->
				<div class="card bg-transparent">
					<!-- Card header -->
					<div class="card-header border-bottom bg-transparent px-0 pt-0">
						<h3 class="mb-0">About This Hotel</h3>
					</div>

					<!-- Card body START -->
					<div class="card-body pt-4 p-0">

						<p class="mb-3">@Model.Desc.Substring(0, Model.Desc.Length / 2)</p>

						<div class="collapse" id="collapseContent">
							<p class="my-3">@Model.Desc.Substring(Model.Desc.Length / 2)</p>
						</div>


						<a class="p-0 mb-4 mt-2 btn-more d-flex align-items-center collapsed" data-bs-toggle="collapse" href="#collapseContent" role="button" aria-expanded="false" aria-controls="collapseContent">
							See <span class="see-more ms-1">more</span><span class="see-less ms-1">less</span><i class="fa-solid fa-angle-down ms-2"></i>
						</a>

						<!-- List -->
						<h5 class="fw-light mb-2">Advantages</h5>
						<ul class="list-group list-group-borderless mb-0">
							@foreach (var item in Model.AdvantageNames)
							{
								<li class="list-group-item h6 fw-light d-flex mb-0"><i class="bi bi-patch-check-fill text-success me-2"></i>@item</li>
							}

						</ul>
					</div>
					<!-- Card body END -->
				</div>
				<!-- About hotel START -->
				<!-- Amenities START -->
				<div class="card bg-transparent">
