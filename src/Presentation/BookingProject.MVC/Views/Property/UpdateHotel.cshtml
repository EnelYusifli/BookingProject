														<!-- Listing Name -->
														<div class="col-12">
															<label class="form-label">Listing name *</label>
															<input class="form-control" type="text" placeholder="Enter property name" asp-for="@Model.Name">
															<span asp-validation-for="@Model.Name" class="text-danger"></span>

															<small>A catchy name usually includes: House name - Room name - A tourist destination</small>
														</div>

													</div>
												</div>
												<!-- Card body END -->
											</div>
											<!-- Listing category END -->

											<!-- Listing location START -->
											<div class="card shadow">
												<!-- Card header -->
												<div class="card-header border-bottom">
													<h5 class="mb-0">Property Location</h5>
												</div>
			
												<!-- Card body START -->
												<div class="card-body">
													<div class="row g-3">
														<!-- Country -->
														<div class="col-12">
															<label class="form-label">Choose property country *</label>
															<select class="form-select js-choice" asp-for="@Model.CountryId">
																<option value="">Select country</option>
																@foreach (var item in vm.Countries)
																{
																	<option value="@item.Id">@item.CountryName</option>
																}

															</select>
															<span asp-validation-for="@Model.CountryId" class="text-danger"></span>
														</div>
														<!-- City -->
														<div class="col-md-6">
															<label class="form-label">City *</label>
															<input class="form-control" type="text" placeholder="Enter city" asp-for="@Model.City">
															<span asp-validation-for="@Model.City" class="text-danger"></span>

														</div>
														<!-- Address -->
														<div class="col-12">
															<label class="form-label">Address *</label>
															<input class="form-control" type="text" placeholder="Enter address" asp-for="@Model.Address">
															<span asp-validation-for="@Model.Address" class="text-danger"></span>

														</div>
