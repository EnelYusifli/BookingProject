	<script>
		function updateDeletedImageIds() {
			let selectedImageIds = [];
			// Select all checkboxes with the class 'form-check-input'
			document.querySelectorAll('.form-check-input:checked').forEach(checkbox => {
				selectedImageIds.push(checkbox.value);
			});

			// Set the value of the hidden input field
			document.getElementById('DeletedImageIds').value = selectedImageIds.join(',');
		}

		// Ensure the hidden field is updated whenever a checkbox is clicked
		document.querySelectorAll('.form-check-input').forEach(checkbox => {
			checkbox.addEventListener('change', updateDeletedImageIds);
		});
